query getPratosByEstabelecimentoID($estabelecimentoID: Int!) {
  cadastroPrato(where: {idEstabelecimento_cadastroEstabelecimento: {_eq: $estabelecimentoID}}) {
    ativoPrato
    avaliacaoPrato
    descricaoPrato
    fotoPrato
    idEstabelecimento_cadastroEstabelecimento
    idPrato
    nomePrato
    valorPrato
    categoria_Categoria
  }
}

{"estabelecimentoID": 1}

